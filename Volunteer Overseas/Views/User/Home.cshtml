    <div class="container-fluid Banner">
        <div class="Banner-header">Discover Yourself, Discover the World</div>
        <div class="banner-description">Apply to your perfect volunteer, intern, or teach abroad program</div>
        <div class="heading">
            <div class="form-row1">
                <div>Type</div>
                <div>Location</div>
                <div>Activity</div>
            </div>
        </div>
        <div class="form-section">
            <form>

                <div class="header-content">
                    <div class="mb-3">
                        <span class="formobile">Type</span>
                        <span>
                            <select class="volunteer form-control transparent-input">
                                <option value="">Pick a theme. Any theme</option>
                                <option value="default">Default Theme</option>
                                <option value="black">Dark Gloss</option>
                                <option value="orange">Light Gloss</option>
                            </select>
                        </span>
                    </div>
                    <div class="location mb-3">
                        <span class="formobile">Location</span>
                        <span class="anywhere"> <input type="text" name="" class="form-control transparent-input anywhere" placeholder="Anywhere" id="auto-location"></span>
                    </div>
                    <div class="mb-3">
                        <span class="formobile">Activity</span>
                        <span>
                            <select class="anything form-control transparent-input">
                                <option value="">Pick a theme. Any theme</option>
                                <option value="default">Default Theme</option>
                                <option value="black">Dark Gloss</option>
                                <option value="orange">Light Gloss</option>
                            </select>
                        </span>
                    </div>
                    <div class="submit">
                        <button class="form-control ">Search</button>
                    </div>
                </div>
            </form>
        </div>
    </div>

